{
  "name": "chainqna",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env && ng build --aot --prod",
    "start": "ng serve --proxy-config proxy.conf.json",
    "lint": "ng lint --type-check && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "ng test",
    "test:ci": "npm run lint && ng test --single-run --code-coverage",
    "e2e": "ng e2e",
    "env": "env2json npm_package_version -o src/environments/.env.json",
    "generate": "ng generate",
    "prepush": "npm run lint"
  },
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngx-translate/core": "^7.0.0",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.3",
    "@ngrx/store": "2.2.2",
    "@ngrx/store-devtools": "3.2.4",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.3",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "^2.5.52",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.52",
    "@types/node": "^8.0.0",
    "codelyzer": "~3.1.1",
    "env2json": "^1.0.0",
    "htmlhint": "^0.9.13",
    "https-proxy-agent": "^1.0.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "~1.7.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "phantomjs-prebuilt": "^2.1.14",
    "protractor": "~5.1.2",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^16.0.0",
    "ts-node": "^3.1.0",
    "tslint": "~5.4.3",
    "typescript": "~2.3.3"
  }
}
