{
  "name": "chainqna",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env && ng build --aot --prod",
    "start": "ng serve --proxy-config proxy.conf.json",
    "lint": "ng lint --type-check && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "ng test",
    "test:ci": "npm run lint && ng test --single-run --code-coverage",
    "e2e": "ng e2e",
    "env": "env2json npm_package_version -o src/environments/.env.json",
    "generate": "ng generate",
    "prepush": "npm run lint"
  },
  "dependencies": {
    "@angular/animations": "^5.0.0-beta.6",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "^5.0.0-beta.6",
    "@angular/compiler": "^5.0.0-beta.6",
    "@angular/core": "^5.0.0-beta.6",
    "@angular/forms": "^5.0.0-beta.6",
    "@angular/http": "^5.0.0-beta.6",
    "@angular/material": "^2.0.0-beta.10",
    "@angular/platform-browser": "^5.0.0-beta.6",
    "@angular/platform-browser-dynamic": "^5.0.0-beta.6",
    "@angular/router": "^5.0.0-beta.6",
    "@ngrx/effects": "^4.0.0",
    "@ngrx/router-store": "^4.0.0",
    "@ngrx/store": "^4.0.0",
    "@ngrx/store-devtools": "^4.0.0",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.2",
    "web-animations-js": "^2.2.5",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "~1.4.0-rc.2",
    "@angular/compiler-cli": "^5.0.0-beta.6",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "^8.0.0",
    "codelyzer": "~3.1.1",
    "env2json": "^1.0.0",
    "htmlhint": "^0.9.13",
    "https-proxy-agent": "^1.0.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "~1.7.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "phantomjs-prebuilt": "^2.1.14",
    "protractor": "~5.1.2",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^16.0.0",
    "ts-node": "^3.1.0",
    "tslint": "~5.4.3",
    "typescript": "~2.3.3"
  }
}
